{
  "name": "gov-budget-report",
  "version": "0.1.0",
  "private": true,
  "type": "commonjs",
  "scripts": {
    "dev": "node src/index.js",
    "start": "node src/index.js",
    "db:migrate": "node-pg-migrate up -m db/migrations",
    "db:rollback": "node-pg-migrate down -m db/migrations --count 1",
    "db:status": "node-pg-migrate up -m db/migrations --dry-run",
    "db:clean-mojibake:dry": "node scripts/clean_mojibake_db.js --dry-run",
    "db:clean-mojibake:apply": "node scripts/clean_mojibake_db.js --apply",
    "test": "jest --runInBand",
    "test:parser": "jest --runInBand tests/excelFiller.test.js tests/pdfPreflightService.test.js",
    "test:parser:full": "jest --runInBand tests/excelFiller.test.js tests/pdfPreflightService.test.js tests/validation.test.js tests/lineItems.test.js",
    "test:e2e": "jest --runInBand tests/uiScreenshot.test.js",
    "golden:update": "node scripts/golden/update.js",
    "golden:check": "node scripts/golden/check.js",
    "ui:dev": "vite --config ui/vite.config.ts",
    "ui:build": "vite build --config ui/vite.config.ts",
    "ui:preview": "vite preview --config ui/vite.config.ts --port 4173",
    "ui:screenshot": "node scripts/ui/screenshot.js"
  },
  "dependencies": {
    "axios": "^1.6.7",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.6",
    "dotenv": "^16.4.5",
    "exceljs": "^4.4.0",
    "express": "^4.19.2",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.563.0",
    "multer": "^1.4.5-lts.1",
    "pdf-parse": "^2.4.5",
    "pg": "^8.12.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-dropzone": "^14.2.3",
    "react-router-dom": "^6.26.2",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.18",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.23",
    "jest": "^29.7.0",
    "node-pg-migrate": "^7.3.1",
    "playwright": "^1.46.1",
    "postcss": "^8.5.6",
    "supertest": "^6.3.4",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.5.4",
    "vite": "^5.3.5"
  }
}
